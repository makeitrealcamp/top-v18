{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": [
            "error", {
                "argsIgnorePattern": "next"
            }
        ],
        "implicit-arrow-linebreak": "off"
    }
}
